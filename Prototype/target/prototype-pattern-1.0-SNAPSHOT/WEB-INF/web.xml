<web-app>
    <servlet>
        <servlet-name>PrototypeServlet</servlet-name>
        <servlet-class>com.example.prototype.PrototypeServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>PrototypeServlet</servlet-name>
        <url-pattern>/clone</url-pattern>
    </servlet-mapping>
